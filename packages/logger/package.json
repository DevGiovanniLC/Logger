{
    "name": "@giodev/logger",
    "version": "1.0.0",
    "type": "module",
    "sideEffects": false,
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
