{
    "name": "@giodev/logger",
    "version": "1.0.0",
    "type": "module",
    "sideEffects": false,
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./Transport": {
            "types": "./dist/Transport/index.d.ts",
            "import": "./dist/Transport/index.mjs",
            "require": "./dist/Transport/index.cjs"
        },
        "./Transport/node": {
            "types": "./dist/Transport/node.d.ts",
            "import": "./dist/Transport/node.mjs",
            "require": "./dist/Transport/node.cjs"
        },
        "./Formatter": {
            "types": "./dist/Formatter/index.d.ts",
            "import": "./dist/Formatter/index.mjs",
            "require": "./dist/Formatter/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup",
        "clean": "rimraf dist",
        "test": "vitest",
        "format": "prettier --write \"src/**/*.{ts,html,css,scss,json}\""
    },
    "files": [
        "dist"
    ]
}
